import java.util.ArrayList;

public class DifferenceArraysArrayLists {

    public static void main(String[] args) {
        // Array Example
        int[] intArray = new int[5]; // Declare and initialize an array of size 5
        intArray[0] = 10;
        intArray[1] = 20;
        intArray[2] = 30;
        intArray[3] = 40;
        intArray[4] = 50;

        System.out.println("Array Elements:");
        for (int i = 0; i < intArray.length; i++) {
            System.out.println(intArray[i]);
        }

        // ArrayList Example
        ArrayList<Integer> intArrayList = new ArrayList<>(); // Declare an ArrayList of Integer type
        intArrayList.add(10);
        intArrayList.add(20);
        intArrayList.add(30);
        intArrayList.add(40);
        intArrayList.add(50);

        System.out.println("\nArrayList Elements:");
        for (Integer element : intArrayList) {
            System.out.println(element);
        }
    }
}
